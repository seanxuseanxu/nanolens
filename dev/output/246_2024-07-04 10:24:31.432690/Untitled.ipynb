   "source": [
    "import jax.numpy as jnp\n",
    "def convertEllipticity(e1, e2):\n",
    "    phi = jnp.arctan2(e2, e1) / 2\n",
    "    c = jnp.minimum(jnp.sqrt(e1 ** 2 + e2 ** 2), 0.9999)\n",
    "    q = (1 - c) / (1 + c)\n",
    "    return float(q), float(phi)"
   ]
